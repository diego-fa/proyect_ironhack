<h1>Show Estate</h1>
<p>
   <strong>Title:</strong>
   <%= @estate.size %>
</p>

<p>
   <strong>City:</strong>
   <%= @estate.description %>
</p>

<p>
   <strong>latitude:</strong>
   <%= @estate.latitude %>
</p>

<p>
   <strong>longitude:</strong>
   <%= @estate.longitude %>
</p>


 <div class="col-md-6">
   <div id="map" style='width: 100%; height: 400px;'></div>
 </div>
</div>

<script type="text/javascript">
 var map;
function initMap() {
  map = new google.maps.Map(document.getElementById('map'), {
    center: {lat: -34.397, lng: 150.644},
    zoom: 8
  });
}
</script>

<script async defer
   src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCYBftC_sD8wPk6SpZaYUGA7C9wpzsd6uo&callback=initMap">
</script>



